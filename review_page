function formitems(which) {
	var disableval = true;
	if(which === 'standard') {
		disableval = false;
	}
	$('#audio_channel').prop("disabled", disableval);
	$('#length_adj').prop("disabled", disableval);
	$('#offset_audio').prop("disabled", disableval);
	$('#offset_start').prop("disabled", disableval);
	if(which === 'comments') {
		disableval = false;
	} else {
		disableval = true;
	}
	$('#comment_text').prop("disabled", disableval);
	if(which === 'standard') {
		$('#length_adj').val(sreview_viddata.get_length_adjust());
		$('#offset_start').val(sreview_viddata.get_start_offset());
	}
}
function setPoint(vid, startstop, time) {
	sreview_viddata.set_point(vid, startstop, time);
	$('#standard').prop("checked", true);
	formitems('standard');
};
function edit_offset_start(val) {
	sreview_viddata.set_start_offset(val);
};
function edit_length_adj(val) {
	sreview_viddata.set_length_adj(val);
};
</script>
<p>This video is for the talk entitled <q><strong>Intro to the Software Defined Radio Track</strong></q> by <strong>Philip Balister, Martin Braun and Sylvain Munaut</strong> which happened in room <strong>AW1.120</strong> from <strong>2017-02-04 10:30:00+01</strong> to <strong>2017-02-04 10:40:00+01</strong> at <strong>FOSDEM 2017</strong></p>
<p>To be able to see this preview in your browser, you must use an HTML5 browser which supports the WebM format. Alternatively, here is a <a href='/video/1/2017-02-04/A/introsdr.mp4'>direct download <span class="glyphicon glyphicon-download"></span></a> of the same file.</p>
